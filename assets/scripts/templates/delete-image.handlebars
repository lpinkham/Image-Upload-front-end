<div class="row">
  {{#each images as |image|}}
    {{#if image.editable}}
    <div class="col-sm-6 row">
      <div class="col-1"></div>
      <div class="col-10 justify-content-center handlebar-card">
        <img src="{{image.url}}" style="max-width: 100%; max-height: 100%;"/>
        <h5>Name: {{image.name}}</h5>
        <h6>Owner: {{image.userName}}</h6>
        <h6>Tag:</h6>
          <ul>
              {{#each tag as |tag|}}
              {{#if tag}}
              <li>{{tag}}</li>
              {{/if}}
              {{/each}}
          </ul>
        <h6>Date Created: {{image.createdAt}}</h6>
        <h6>Date Modified: {{image.updatedAt}}</h6>
        <button class="delete-image btn btn-danger delete col-mb-3" data-del-image="{{image._id}}">Delete Image</button>
      </div>
      <div class="col-1"></div>
    </div>
    {{/if}}
  {{/each}}
</div>
